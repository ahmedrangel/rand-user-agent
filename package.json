{
  "name": "@ahmedrangel/rand-user-agent",
  "version": "2.0.18",
  "description": "This is an adapted version of rand-user-agent by WebScrapingAPI specifically designed for environments lacking access to server file systems (fs), such as Cloudflare Workers.",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "node example/example.js",
    "build": "shx rm -rf ./dist/* && tsup-node src/index.ts --format esm --dts --config tsconfig.json"
  },
  "engines": {
    "node": ">=17.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedrangel/rand-user-agent.git"
  },
  "homepage": "https://github.com/ahmedrangel/rand-user-agent#readme",
  "type": "module",
  "keywords": [
    "user-agent",
    "user",
    "agent",
    "nodejs",
    "agents",
    "user-agents",
    "random",
    "random-user-agent",
    "randomizer",
    "cloudflare-workers"
  ],
  "author": "ahmedrangel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahmedrangel/rand-user-agent/issues"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/node": "^22.5.0",
    "@typescript-eslint/parser": "^8.2.0",
    "changelogen": "^0.5.5",
    "eslint": "^9.9.1",
    "shx": "^0.3.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "changelog": {
    "repo": {
      "repo": "ahmedrangel/rand-user-agent",
      "provider": "github",
      "domain": "github.com"
    }
  },
  "packageManager": "pnpm@9.8.0"
}